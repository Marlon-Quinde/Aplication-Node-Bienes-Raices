{
  "name": "bienes-raices-mvc",
  "version": "1.0.0",
  "description": "Mi primer proyecto de nodejs",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "css": "postcss public/css/tailwind.css -o public/css/app.css --watch",
    "js": "webpack --watch",
    "tsc": "tsc",
    "watch": "concurrently \"npm run js\" \"npm run css\"",
    "db:importar": "ts-node ./src/seed/seeder.ts -i",
    "db:eliminar": "ts-node ./src/seed/seeder.ts -e"
  },
  "keywords": [
    "MVC",
    "Pug",
    "Tailwing",
    "MySql",
    "Sequelize"
  ],
  "author": "Marlon Quinde",
  "license": "ISC",
  "dependencies": {
    "@dr.pogodin/csurf": "^1.13.0",
    "@types/csrf": "^3.1.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "dropzone": "^5.9.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.1",
    "mysql2": "^3.6.0",
    "nodemailer": "^6.9.4",
    "pug": "^3.0.2",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/nodemailer": "^6.4.9",
    "autoprefixer": "^10.4.15",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.28",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
